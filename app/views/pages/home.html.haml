- provide :title, @page.page_title
- provide :meta, @page.meta_description

- content_for :full_width do
  #banner_wrapper.home_banner
    = page_banner @page
.page_columns
  .main_column
    = raw @page.body
  .side_column
    %iframe{:allowfullscreen => "", :frameborder => "0", :height => "400", :src => "https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d38702.785939379675!2d1.164788!3d52.679408!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0xe7aa13dc58c5c5fe!2sSwan+Inn!5e0!3m2!1sen!2suk!4v1463873452306", :style => "border:0", :width => "100%"}
.gallery_thumbs
  - Page.featured.each do |page|
    .gallery_thumb
      = image_tag page.banner_images.first.image.featured
      %h4= link_to page.name, page
      %span= summary(page)
