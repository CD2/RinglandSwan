- provide :title, @page.page_title
- provide :meta, @page.meta_description
- content_for :full_width do
  = page_banner @page

.central_column
  = raw @page.body

.gallery_thumbs
  - GalleryImage.all.each do |image|
    .gallery_thumb
      = link_to image_tag(image.image.gallery_thumbnail), image.image.url, data: {lightbox: "image"}
